err-msgs.h: err.c Makefile.err-msgs-h
	${_MKTARGET_CREATE}
	sp='[[:space:]]*'; \
	from="^$$sp\(\".*\"\)\,$$sp/\*$$sp\([0-9][0-9]*\)$$sp\*/\$$"; \
	${TOOL_SED} -n -e "s,$$from,#define MSG_\2 \1,p" < err.c > ${.TARGET}.tmp
	mv -f ${.TARGET}.tmp ${.TARGET}

CLEANFILES+=	err-msgs.h
DPSRCS+=	err-msgs.h

externs1.h: err-msgs.h
